<Application x:Class="Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:Client.Utils.Converters"
             Startup="OnStartup">
    <Application.Resources>
        <converters:RepsSetConverter x:Key="RepsSetConverter" />
        <converters:WeightSetConverter x:Key="WeightSetConverter" />
        <converters:ExerciseNameConverter x:Key="ExerciseNameConverter" />
        <converters:DateTimeConverter x:Key="DateTimeConverter" />
        <converters:IndexConverter x:Key="IndexConverter" />
    </Application.Resources>
</Application>
