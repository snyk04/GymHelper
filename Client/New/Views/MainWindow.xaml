        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="GymHelper" Height="1080" Width="1920" Left="0" Top="0" WindowState="Maximized">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Content="История тренировок" Width="150" Click="OpenWorkoutStoryWindow"></Button>
        <Button Content="Упражнения" Width="150" Click="OpenExerciseListWindow"></Button>
        <Button Content="Статистика" Width="150" Click="OpenStatisticsWindow"></Button>
    </StackPanel>
</Window>
